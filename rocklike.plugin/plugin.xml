<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         point="org.eclipse.ui.commands">
      <category
            name="Hong Category"
            id="rocklike.plugin.commands.category">
      </category>
      <command
            name="Hong Command"
            categoryId="rocklike.plugin.commands.category"
            id="rocklike.plugin.commands.sampleCommand">
      </command>
      <command
            id="rocklike.plugin.bookmark.createBookmarkCommand"
            name="bookmark 생성">
         <commandParameter
               id="rocklike.plugin.bookmark.commandParameter_number"
               name="key value"
               optional="true">
         </commandParameter>
      </command>
      <command
            id="rocklike.plugin.bookmark.gotoBookmarkCommand"
            name="bookmark 가기">
         <commandParameter
               id="rocklike.plugin.bookmark.commandParameter_number"
               name="key value"
               optional="true">
         </commandParameter>
      </command>
      <command
            defaultHandler="rocklike.plugin.explorer.OpenExplorer"
            id="rocklike.plugin.explorer.OpenExplorer"
            name="탐색기 열기">
      </command>
      <command
            defaultHandler="rocklike.plugin.explorer.CopyFilePath"
            id="rocklike.plugin.explorer.CopyFilePath"
            name="파일경로 복사">
      </command>
      <command
            id="mybatisXmlGotoCommand"
            name="Mybatis Xml 열기">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            commandId="rocklike.plugin.commands.sampleCommand"
            class="rocklike.plugin.handlers.SampleHandler">
      </handler>
      <handler
            class="rocklike.plugin.bookmark.HongCreateBookmarkHandler"
            commandId="rocklike.plugin.bookmark.createBookmarkCommand">
      </handler>
      <handler
            class="rocklike.plugin.bookmark.HongGotoBookmark"
            commandId="rocklike.plugin.bookmark.gotoBookmarkCommand">
      </handler>
      <handler
            class="rocklike.plugin.mybatis.MybatisXmlGotoHandler"
            commandId="mybatisXmlGotoCommand">
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.bindings">
      <key
            commandId="rocklike.plugin.bookmark.createBookmarkCommand"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M1+M2+0">
         <parameter
               id="rocklike.plugin.bookmark.commandParameter_number"
               value="0">
         </parameter>
      </key>
      <key
            commandId="rocklike.plugin.bookmark.createBookmarkCommand"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M1+M2+1">
         <parameter
               id="rocklike.plugin.bookmark.commandParameter_number"
               value="1">
         </parameter>
      </key>
      <key
            commandId="rocklike.plugin.bookmark.createBookmarkCommand"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M1+M2+2">
         <parameter
               id="rocklike.plugin.bookmark.commandParameter_number"
               value="2">
         </parameter>
      </key>
      <key
            commandId="rocklike.plugin.bookmark.createBookmarkCommand"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M1+M2+3">
         <parameter
               id="rocklike.plugin.bookmark.commandParameter_number"
               value="3">
         </parameter>
      </key>
      <key
            commandId="rocklike.plugin.bookmark.createBookmarkCommand"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M1+M2+4">
         <parameter
               id="rocklike.plugin.bookmark.commandParameter_number"
               value="4">
         </parameter>
      </key>
      <key
            commandId="rocklike.plugin.bookmark.createBookmarkCommand"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M1+M2+5">
         <parameter
               id="rocklike.plugin.bookmark.commandParameter_number"
               value="5">
         </parameter>
      </key>
      <key
            commandId="rocklike.plugin.bookmark.createBookmarkCommand"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M1+M2+6">
         <parameter
               id="rocklike.plugin.bookmark.commandParameter_number"
               value="6">
         </parameter>
      </key>
      <key
            commandId="rocklike.plugin.bookmark.createBookmarkCommand"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M1+M2+7">
         <parameter
               id="rocklike.plugin.bookmark.commandParameter_number"
               value="7">
         </parameter>
      </key>
      <key
            commandId="rocklike.plugin.bookmark.createBookmarkCommand"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M1+M2+8">
         <parameter
               id="rocklike.plugin.bookmark.commandParameter_number"
               value="8">
         </parameter>
      </key>
      <key
            commandId="rocklike.plugin.bookmark.createBookmarkCommand"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M1+M2+9">
         <parameter
               id="rocklike.plugin.bookmark.commandParameter_number"
               value="9">
         </parameter>
      </key>



      <key
            commandId="rocklike.plugin.bookmark.gotoBookmarkCommand"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M3+0">
         <parameter
               id="rocklike.plugin.bookmark.commandParameter_number"
               value="0">
         </parameter>
      </key>
      <key
            commandId="rocklike.plugin.bookmark.gotoBookmarkCommand"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M3+1">
         <parameter
               id="rocklike.plugin.bookmark.commandParameter_number"
               value="1">
         </parameter>
      </key>

      <key
            commandId="rocklike.plugin.bookmark.gotoBookmarkCommand"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M3+2">
         <parameter
               id="rocklike.plugin.bookmark.commandParameter_number"
               value="2">
         </parameter>
      </key>

      <key
            commandId="rocklike.plugin.bookmark.gotoBookmarkCommand"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M3+3">
         <parameter
               id="rocklike.plugin.bookmark.commandParameter_number"
               value="3">
         </parameter>
      </key>

      <key
            commandId="rocklike.plugin.bookmark.gotoBookmarkCommand"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M3+4">
         <parameter
               id="rocklike.plugin.bookmark.commandParameter_number"
               value="4">
         </parameter>
      </key>

      <key
            commandId="rocklike.plugin.bookmark.gotoBookmarkCommand"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M3+5">
         <parameter
               id="rocklike.plugin.bookmark.commandParameter_number"
               value="5">
         </parameter>
      </key>

      <key
            commandId="rocklike.plugin.bookmark.gotoBookmarkCommand"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M3+6">
         <parameter
               id="rocklike.plugin.bookmark.commandParameter_number"
               value="6">
         </parameter>
      </key>

      <key
            commandId="rocklike.plugin.bookmark.gotoBookmarkCommand"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M3+7">
         <parameter
               id="rocklike.plugin.bookmark.commandParameter_number"
               value="7">
         </parameter>
      </key>

      <key
            commandId="rocklike.plugin.bookmark.gotoBookmarkCommand"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M3+8">
         <parameter
               id="rocklike.plugin.bookmark.commandParameter_number"
               value="8">
         </parameter>
      </key>

      <key
            commandId="rocklike.plugin.bookmark.gotoBookmarkCommand"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M3+9">
         <parameter
               id="rocklike.plugin.bookmark.commandParameter_number"
               value="9">
         </parameter>
      </key>
      <key
            commandId="mybatisXmlGotoCommand"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M1+M2+M3+H">
      </key>
      <!--
      <key
            commandId="rocklike.plugin.commands.sampleCommand"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M1+6">
      </key>
      -->

   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="menu:org.eclipse.ui.main.menu?after=additions">
         <menu
               label="H"
               mnemonic="M"
               id="rocklike.plugin.menus.sampleMenu">
            <command
                  commandId="mybatisXmlGotoCommand"
                  label="테스트"
                  mnemonic="S">
            </command>
         </menu>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="popup:org.eclipse.jdt.ui.PackageExplorer">
         <menu
               label="홍">
            <command
                  commandId="rocklike.plugin.explorer.OpenExplorer"
                  label="탐색기 열기"
                  style="push">
            </command>
            <command
                  commandId="rocklike.plugin.explorer.CopyFilePath"
                  label="파일경로 복사"
                  style="push">
            </command>
         </menu>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="popup:org.eclipse.ui.navigator.ProjectExplorer#PopupMenu">
         <menu
               label="홍">
            <command
                  commandId="rocklike.plugin.explorer.OpenExplorer"
                  label="탐색기 열기"
                  style="push">
            </command>
            <command
                  commandId="rocklike.plugin.explorer.CopyFilePath"
                  label="파일경로 복사"
                  style="push">
            </command>
         </menu>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="popup:org.eclipse.ui.views.ResourceNavigator">
         <menu
               label="홍">
            <command
                  commandId="rocklike.plugin.explorer.OpenExplorer"
                  label="탐색기 열기"
                  style="push">
            </command>
            <command
                  commandId="rocklike.plugin.explorer.CopyFilePath"
                  label="파일경로 복사"
                  style="push">
            </command>
         </menu>
      </menuContribution>
